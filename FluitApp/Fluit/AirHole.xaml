<UserControl x:Class="Fluit.AirHole"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">
    
    <Grid>
        <Ellipse x:Name="AirHoleEllipse" Height="90" Canvas.Left="{Binding Height, ElementName=AirHoleEllipse, Converter={StaticResource DevideConverter}, ConverterParameter='-0.5'}" Width="{Binding Height, ElementName=AirHoleEllipse }" />
    </Grid>
</UserControl>
